    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-vue-jsx": "^3.2.0",
    "babel-preset-flow-vue": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "buble": "^0.19.8",
